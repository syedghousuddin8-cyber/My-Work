# Production Environment Configuration Example
# Copy this file to .env.production and update with your actual values

# API Configuration
VITE_API_URL=http://52.66.71.133/api/v1
VITE_WS_URL=ws://52.66.71.133/ws

# Environment
VITE_ENV=production

# App Configuration
VITE_APP_NAME=Delivery Platform Admin
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# External Services (Update with your actual keys)
# VITE_GOOGLE_MAPS_KEY=your_google_maps_api_key
# VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key

# Socket.IO Configuration
VITE_SOCKET_RECONNECTION_ATTEMPTS=5
VITE_SOCKET_RECONNECTION_DELAY=1000

# Upload Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/jpg,application/pdf

# Pagination
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# Session Configuration
VITE_SESSION_TIMEOUT=3600000
VITE_IDLE_TIMEOUT=1800000

# Performance
VITE_ENABLE_PWA=false
VITE_ENABLE_SERVICE_WORKER=false
